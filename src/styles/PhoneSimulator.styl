@import 'nib/vendor'
@import 'nib/positions'
@import 'nib/overflow'
.phonesimulator
  .wrapper
    relative: 0 0
    width 100%
    margin auto
    padding-top calc(16/9 * 100%)
    .content
      absolute: top 0 left 0
      width 100%
      height 100%
      background #aaa
      border-radius: 25px
